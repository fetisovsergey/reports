$("#like<%= @post.id %>").html("<%= j render 'microposts/dislike', :micropost => @post %>")
$("#count_likes_of_micropost<%= @post.id %>").html("<% if @post.votes.count != 0 %><%= @post.votes.count %><% end %>")
$("#voters_of_<%= @post.id %>").html("<%= j render 'microposts/users_who_like', :micropost => @post %>")